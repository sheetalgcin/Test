pipeline {
agent any
  stages {
    stage('Robot tests') {
      agent {
        dockerfile {
          filename 'Docker'
          label 'docker'

          args "-v $WORKSPACE:/workspace"
          additionalBuildArgs "--no-cache"
        }
      }
      steps {
        sh 'docker --version'
        sh "docker run robot"
      }
    }

  }

}

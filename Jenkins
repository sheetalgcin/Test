pipeline {
agent { any }
  stages {
    stage('Robot tests') {
      agent {
        dockerfile {
          filename 'Docker'
          label 'docker'
        }
      }
      steps {
        sh 'echo $PATH'
        sh "/Applications/Docker.app/Contents/Resources/bin/docker --version"
        sh "/Applications/Docker.app/Contents/Resources/bin/docker run robot"
      }
    }

  }

}
